										<Text text="{company}"></Text>
									</tab:template>
								</tab:Column>
									<tab:Column>
									<tab:label>
										<Label text="company"></Label>
									</tab:label>
									<tab:template>
										<Text text="{company}"></Text>
									</tab:template>
								</tab:Column>
									<tab:Column>
