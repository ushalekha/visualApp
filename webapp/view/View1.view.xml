										<Text text="{phone}"></Text>
									</tab:template>
								</tab:Column>
									<tab:Column>
									<tab:label>
										<Label text="Emailid"></Label>
									</tab:label>
									<tab:template>
										<Text text="{email}"></Text>
									</tab:template>
								</tab:Column>
									<tab:Column>
									<tab:label>
										<Label text="city"></Label>
									</tab:label>
									<tab:template>
										<Text text="{city}"></Text>
									</tab:template>
								</tab:Column>
									<tab:Column>
									<tab:label>
										<Label text="company"></Label>
									</tab:label>
									<tab:template>
										<Text text="{company}"></Text>
									</tab:template>
								</tab:Column>
									<tab:Column>
